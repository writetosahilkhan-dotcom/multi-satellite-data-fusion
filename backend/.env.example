# Environment Configuration
# Copy this file to .env and fill in your API keys

# Gemini AI API Key (optional - for AI-powered insights)
# Get it from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Enable/Disable AI Insights
ENABLE_AI_INSIGHTS=true

# N2YO Satellite Tracking API (optional)
# Get it from: https://www.n2yo.com/api/
N2YO_API_KEY=your_n2yo_api_key_here

# Server Configuration
BACKEND_PORT=8000
FRONTEND_PORT=3000

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Database
DATABASE_URL=sqlite+aiosqlite:///./satellite_data.db
